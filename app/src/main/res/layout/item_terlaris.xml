<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/list_item_terlaris"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp">


    <TextView
        android:id="@+id/nama_terlaris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="nama_produk"></TextView>

    <TextView

        android:id="@+id/jumlah_terlaris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/nama_terlaris"
        android:padding="5dp"
        android:text="jumlah"></TextView>


</RelativeLayout>